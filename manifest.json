{
  "name": "GitHub PR master notice",
  "version": "1",
  "manifest_version": 2,
  "description": "GitHub show merge branch top of merge button",
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": ["script.js"],
    "run_at": "document_idle"
  }],
  "icons": {
    "16":  "icons/icon16.png",
    "48":  "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
